{
    "env": {
        "browser": true,
        "es6": true
    },
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "extends": "prettier",
    "parser": "babel-eslint",
    "plugins": [
        "prettier",
        "react"
    ],
    "rules": {
        "prettier/prettier": "error",
        "no-trailing-spaces": "error",
        "no-console": 1
    }
}